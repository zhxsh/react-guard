{
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaVersion": 8,//ECMAScript syntax 最新版本
        "ecmaFeatures": {
            "impliedStrict": true,
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "React": false,
        "jQuery": false,
        "$": false
    },
    "rules": {
        "no-dupe-keys": "off",
        "no-var": "off",
        "complexity": "off",
        "indent": "off"
        
    }
}